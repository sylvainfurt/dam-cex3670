<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<groupHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <group>
        <name>Application Users</name>
        <securityMap>SECURITYMAP_PERSONAL</securityMap>
        <uuid>SYSTEM_GROUP_ROLE_APPLICATION_USERS</uuid>
        <groupTypeUuid>SYSTEM_GROUP_TYPE_CUSTOM</groupTypeUuid>
        <description>Members of this group will be able to use applications exposed through the applications portal, but do not have permission to use the designer interface.</description>
        <delegatedCreation>false</delegatedCreation>
        <memberPolicy>MEMBERPOLICY_CLOSED</memberPolicy>
        <viewingPolicy>VIEWINGPOLICY_HIGH</viewingPolicy>
        <attributes/>
    </group>
    <members>
        <users/>
        <groups/>
    </members>
    <admins>
        <users>
            <userUuid>Administrator</userUuid>
        </users>
        <groups/>
    </admins>
    <ruleSet>
        <expressions>
            <entityTypeName>u</entityTypeName>
            <conditions>
                <attributeName>userTypeName</attributeName>
                <operatorId>1</operatorId>
                <value>User</value>
            </conditions>
        </expressions>
    </ruleSet>
</groupHaul>
